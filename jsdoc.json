{
  "tags": {
    "allowUnknownTags": [
      "optional",
      "component"
    ]
  },
  "plugins": [
    "node_modules/better-docs/typescript",
    "node_modules/better-docs/component"
  ],
  "source": {
    "includePattern": "\\.(jsx|js|ts|tsx|vue$)$",
    "include": [
      "./src"
    ]
  },
  "opts": {
    "destination": "./docs",
    "recurse": true,
    "template": "node_modules/better-docs"
  }
}
